<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="org.d3if4018.convertyounit.fragment.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_jarakFragment"
            app:destination="@id/jarakFragment" />
        <action
            android:id="@+id/action_homeFragment_to_beratFragment"
            app:destination="@id/beratFragment" />
        <action
            android:id="@+id/action_homeFragment_to_suhuFragment"
            app:destination="@id/suhuFragment" />
        <action
            android:id="@+id/action_homeFragment_to_volumeFragment"
            app:destination="@id/volumeFragment" />
    </fragment>
    <fragment
        android:id="@+id/jarakFragment"
        android:name="org.d3if4018.convertyounit.fragment.JarakFragment"
        android:label="fragment_jarak"
        tools:layout="@layout/fragment_jarak" >
        <action
            android:id="@+id/action_jarakFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/beratFragment"
        android:name="org.d3if4018.convertyounit.fragment.BeratFragment"
        android:label="fragment_berat"
        tools:layout="@layout/fragment_berat" >
        <action
            android:id="@+id/action_beratFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/suhuFragment"
        android:name="org.d3if4018.convertyounit.fragment.SuhuFragment"
        android:label="fragment_suhu"
        tools:layout="@layout/fragment_suhu" >
        <action
            android:id="@+id/action_suhuFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/volumeFragment"
        android:name="org.d3if4018.convertyounit.fragment.VolumeFragment"
        android:label="fragment_volume"
        tools:layout="@layout/fragment_volume" >
        <action
            android:id="@+id/action_volumeFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>